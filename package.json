{
  "name": "gangster.css",
  "version": "0.0.01-alpha",
  "description": "Gangster CSS is RWD, semantic, mobile-first, flex-based CSS framework",
  "main": "/js/index.js",
  "scripts": {
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanxcursed/gangster.css.git"
  },
  "keywords": [
    "css",
    "sass",
    "scss",
    "framework",
    "rwd",
    "responsive",
    "flex",
    "flex-based"
  ],
  "author": "Juan Felipe <juan@marriedgames.com.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juanxcursed/gangster.css/issues"
  },
  "homepage": "https://github.com/juanxcursed/gangster.css#readme",
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.14",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.17.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.6"
  },
  "dependencies": {}
}
